<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox spacing="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.projet.controllers.HistoriqueController">
    <TableView fx:id="tableHistorique">
        <columns>
            <TableColumn fx:id="colId" text="ID" />
            <TableColumn fx:id="colUtilisateurId" text="Utilisateur ID" />
            <TableColumn fx:id="colDescription" text="Description" />
            <TableColumn fx:id="colDate" text="Date" />
        </columns>
    </TableView>
</VBox>
